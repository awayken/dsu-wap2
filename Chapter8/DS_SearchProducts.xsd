<?xml version="1.0" standalone="yes"?>
<xs:schema id="DS_SearchProducts" targetNamespace="http://www.tempuri.org/DS_SearchProducts.xsd" xmlns:mstns="http://www.tempuri.org/DS_SearchProducts.xsd" xmlns="http://www.tempuri.org/DS_SearchProducts.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="DS_SearchProducts" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Products">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ProductID" type="xs:int" />
              <xs:element name="CategoryID" type="xs:int" minOccurs="0" />
              <xs:element name="SubCategoryID" type="xs:int" />
              <xs:element name="ModelNumber" type="xs:string" minOccurs="0" />
              <xs:element name="ModelName" type="xs:string" minOccurs="0" />
              <xs:element name="ProductImage" type="xs:string" minOccurs="0" />
              <xs:element name="UnitCost" type="xs:decimal" />
              <xs:element name="Description" type="xs:string" minOccurs="0" />
              <xs:element name="Thumbnail" type="xs:string" minOccurs="0" />
              <xs:element name="Availability" type="xs:string" minOccurs="0" />
              <xs:element name="upsize_ts" msdata:ReadOnly="true" type="xs:base64Binary" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Products" />
      <xs:field xpath="mstns:ProductID" />
    </xs:unique>
  </xs:element>
</xs:schema>